{
    "manifest_version": 3,
    "name": "PicTailor",
    "version": "0.0.1",
    "description": "A platform for resizing images for social media platforms with custom dimensions.",
    "permissions": ["activeTab", "storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    }
  }
  
  